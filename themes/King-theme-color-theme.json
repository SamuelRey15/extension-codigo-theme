{
	"name": "King-theme",
	"colors": {
		"editor.background": "#10262c86",	
		"editor.foreground": "#71929a",
		"activityBarBadge.background": "#10262c",
		"sideBarTitle.foreground": "#a7b5ba",
		"activityBar.background": "#00171e", // Cambia el color de fondo de la actividad bar a gris oscuro
		"activityBar.foreground": "#0040ff", // Cambia el color del texto e iconos en la actividad bar a blanco
		"activityBar.inactiveForeground": "#00c3ff", // Cambia el color del texto de elementos inactivos en la actividad bar
		"activityBar.activeBorder": "#ff0000",
		"editorGroupHeader.tabsBackground": "#000405", // Cambia el color de fondo de las pestañas en la zona de archivos a gris oscuro
		"editorGroupHeader.tabsBorder": "#ff0000",
		"editorGroupHeader.border": "#ff0000",
		"sideBar.background": "#00090c", // Cambia el color de fondo de la barra lateral a gris oscuro
		"sideBar.foreground": "#00d9ff", // Cambia el color del texto en la barra lateral a blanco
		"sideBar.border": "#ff0000",
		"titleBar.activeBackground": "#00090c", // Cambia el color de fondo de la barra de título cuando la ventana está activa
    	"titleBar.activeForeground": "#00c3ff", // Cambia el color del texto en la barra de título cuando la ventana está activa
    	"titleBar.inactiveBackground": "#00090c", // Cambia el color de fondo de la barra de título cuando la ventana está inactiva
    	"titleBar.inactiveForeground": "#00c3ff",
		"menu.background": "#00090c", // Cambia el color de fondo del menú emergente a gris oscuro
		"menu.foreground": "#ff0000",// Cambia el color del texto en el menú emergente a blanco
		"menu.selectionBackground": "#00c3ff", // Cambia el color de fondo de la selección en el menú emergente a naranja
		"menu.selectionForeground": "#005103",
		"tab.activeBackground": "#00161d", // Cambia el color de fondo de la pestaña activa a gris oscuro
		"tab.activeForeground": "#00d0ff", // Cambia el color del texto de la pestaña activa a blanco
		"tab.inactiveBackground": "#00090c", // Cambia el color de fondo de las pestañas inactivas a un tono más oscuro
		"tab.inactiveForeground": "#ff0000"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#698e9b"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#2e8cfffe"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#7ab115d1"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",

			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#7ab115d1"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#6882aa"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#c10000"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#e1b000e5"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#06afa1e7"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
